
public class TesteSI {

	public static void main(String[] args) {
		
		Gerente g = new Gerente();
		g.setSenha(2222);
		
		Adm adm = new Adm();
		adm.setSenha(2323);
		
		SistemaInterno si = new SistemaInterno();
		System.out.println("Gerente: " + si.autentica(g));
		System.out.println("Adm: " + si.autentica(adm));

	}

}
